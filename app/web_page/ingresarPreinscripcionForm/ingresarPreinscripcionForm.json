    "formInput" : {
      "type" : "json",
      "value" : [
        "{\r",
        "  \"nuevaFichaInscripcionInput\" : {\r",
        "    \"nombre\" : \"\",\r",
        "    \"apellidos\" : \"\",\r",
        "    \"dni\" : \"\",\r",
        "    \"nuevaFichaInscripcionDocs\" : null,\r",
        "    \"correo\" : \"\"\r",
        "  }\r",
        "}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "return {",
        "\tnuevaFichaInscripcionInput: $data.formInput.nuevaFichaInscripcionInput",
        "}"
      ],
      "exposed" : false
    },
    "procesoAdmisionNombre" : {
      "type" : "expression",
      "value" : [
        "const hoy = new Date();\r",
        "const año = hoy.getFullYear();\r",
        "const mes = hoy.getMonth() + 1; // enero = 0\r",
        "\r",
        "let procAdmision = \"\";\r",
        "\r",
        "if (mes <= 6) {\r",
        "  procAdmision = `${año}-A`;\r",
        "} else {\r",
        "  procAdmision = `${año}-B`;\r",
        "}\r",
        "\r",
        "return procAdmision;"
      ],
      "exposed" : false
    },
    "fechaActual" : {
      "type" : "expression",
      "value" : [
        "var today = new Date();\r",
        "var dd = String(today.getDate()).padStart(2, '0');\r",
        "var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!\r",
        "var yyyy = today.getFullYear();\r",
        "\r",
        "today = dd + '/' + mm + '/' + yyyy;\r",
        "return today;"
      ],
      "exposed" : false
    }
