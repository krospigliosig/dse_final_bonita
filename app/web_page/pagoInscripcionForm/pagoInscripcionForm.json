      "type" : "expression",
      "value" : [
        "var today = new Date();\r",
        "var dd = String(today.getDate()).padStart(2, '0');\r",
        "var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!\r",
        "var yyyy = today.getFullYear();\r",
        "\r",
        "today = dd + '/' + mm + '/' + yyyy;\r",
