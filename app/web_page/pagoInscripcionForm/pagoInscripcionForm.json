    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.transaccionInscripcion ){",
        "\treturn {",
        "\t\t//map transaccionInscripcion variable to expected task contract input",
        "\t\ttransaccionInscripcionInput: {",
        "\t\t\tmontoTotal: $data.transaccionInscripcion.montoTotal !== undefined ? $data.transaccionInscripcion.montoTotal : null,",
        "\t\t\tservicio: $data.transaccionInscripcion.servicio !== undefined ? $data.transaccionInscripcion.servicio : null",
