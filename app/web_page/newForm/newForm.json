    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.datosTransaccion ){",
        "\treturn {",
        "\t\t//map datosTransaccion variable to expected task contract input",
        "\t\tdatosTransaccionInput: {",
        "\t\t\tdni: $data.datosTransaccion.dni !== undefined ? $data.datosTransaccion.dni : null,",
        "\t\t\tmontoTotal: $data.datosTransaccion.montoTotal !== undefined ? $data.datosTransaccion.montoTotal : null,",
        "\t\t\tservicio: $data.datosTransaccion.servicio !== undefined ? $data.datosTransaccion.servicio : null",
